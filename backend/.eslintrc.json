{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "@rocketseat/eslint-config/node",
    "overrides": [
        {
            "files": [
                "*.ts",
                "*.tsx"
            ]
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "new-cap": "off"
  }
}
